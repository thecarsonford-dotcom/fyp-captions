{
  "version": 2,
  "functions": {
    "api/generate-captions.js": {
      "runtime": "nodejs20.x",
      "maxDuration": 15,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
